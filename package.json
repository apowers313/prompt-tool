{
  "name": "prompt-tool",
  "version": "0.1.0",
  "description": "CLI tool for managing AI prompts",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pt": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["cli", "prompt", "ai", "template", "handlebars"],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/prompt-tool.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0-beta.2",
    "@semantic-release/npm": "^13.0.0-beta.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/handlebars": "^4.0.40",
    "@types/node": "^24.2.1",
    "@types/yaml": "^1.9.6",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.0-beta.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.1",
    "chalk": "^5.5.0",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "fs-extra": "^11.3.1",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "minisearch": "^7.1.2",
    "yaml": "^2.8.1"
  }
}
